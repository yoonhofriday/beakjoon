#include <iostream>
using namespace std;

int main()

{
	int A;
	
	cin >> A ;
	
	if (A < 0 || A > 100)
	{
		return 0;
	}
	if (100 >= A && 90 <= A)
	{
		cout << "A";
	}
	if (90 > A && 80 <= A)
	{
		cout << "B";
	}
	if (80 > A && 70 <= A)
	{
		cout << "C";
	}
	if (70 > A && 60 <= A)
	{
		cout << "D";
	}
	if (60 > A)
	{
		cout << "F";
	}
	return 0;
}
